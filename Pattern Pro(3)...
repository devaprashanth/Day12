
#include<stdio.h>
int main() {

        int a;
    scanf("%d",&a);
    
    for(int i=1;i<=a;i++)
    
    {
        for(int b=1;b<=a;b++)
        {
            if((i+b)%2==0)
            {
                printf("1");
    
            }
            else
            {
                printf("0");
            }
        }
        printf("\n");
    }
    
    return 0;
}
